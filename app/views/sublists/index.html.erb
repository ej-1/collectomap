<p id="notice"><%= notice %></p>

<h1>Listing Sublists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sublists.each do |sublist| %>
      <tr>
        <td><%= sublist.title %></td>
        <td><%= sublist.description %></td>
        <td><%= link_to 'Show', sublist %></td>
        <% if current_user %>
          <td><%= link_to 'Edit', edit_sublist_path(sublist) %></td>
          <td><%= link_to 'Destroy', sublist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sublist', new_sublist_path %>
