<p id="notice"><%= notice %></p>

<p>
  <%= image_tag @user.avatar_url(:thumb) %>
</p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Password digest:</strong>
  <%= @user.password_digest %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %>
<%= @user.admin.present? ? (link_to 'Back', users_path) : (link_to 'Back', lists_path) %>
